    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Waterluke",
            "skipFiles": ["<node_internals>/", "node_modules/"],
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run", "dev"],
            "outputCapture": "std",
            "cwd": "${workspaceFolder}",
            "internalConsoleOptions": "openOnSessionStart",
            "stopOnEntry": false
        }
    ]
}
